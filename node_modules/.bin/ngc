#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/src/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/src/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/compiler-cli/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules/@angular/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/@angular+compiler-cli@21.1.2_@angular+compiler@21.1.2_typescript@5.9.3/node_modules:/Users/asagal/Software Projects/openapi-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
else
  exec node  "$basedir/../@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
fi
